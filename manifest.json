{
  "manifest_version": 2,

  "name": "Powfinder",
  "description": "Finding the Goods for the Worthy",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
        "js": ["jquery-3.2.1.min.js", "popup.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Fuck shit up!"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://api.underground.com/"
  ]
}
